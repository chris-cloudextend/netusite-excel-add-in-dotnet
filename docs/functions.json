      "result": {
        "type": "number",
        "dimensionality": "scalar"
      },
      "parameters": [
        {
          "name": "accountType",
          "description": "Account type (e.g., \"OthAsset\") or special type (e.g., \"AcctRec\", \"InvtAsset\")",
          "type": "any",
          "dimensionality": "scalar"
        },
        {
          "name": "fromPeriod",
          "description": "Start period (required for P&L, ignored for BS)",
          "type": "any",
          "dimensionality": "scalar",
          "optional": true
        },
        {
          "name": "toPeriod",
          "description": "End period (required for calculation)",
          "type": "any",
          "dimensionality": "scalar",
          "optional": true
        },
        {
          "name": "subsidiary",
          "description": "Subsidiary name or ID (optional)",
          "type": "any",
          "dimensionality": "scalar",
          "optional": true
        },
        {
          "name": "department",
          "description": "Department filter (optional)",
          "type": "any",
          "dimensionality": "scalar",
          "optional": true
        },
        {
          "name": "location",
          "description": "Location filter (optional)",
          "type": "any",
          "dimensionality": "scalar",
          "optional": true
        },
        {
          "name": "classId",
          "description": "Class filter (optional)",
          "type": "any",
          "dimensionality": "scalar",
          "optional": true
        },
        {
          "name": "accountingBook",
          "description": "Accounting Book ID (optional)",
          "type": "any",
          "dimensionality": "scalar",
          "optional": true
        },
        {
          "name": "useSpecialAccount",
          "description": "Set to 1 to filter by special account type (sspecacct) instead of regular type. For cash flow: AcctRec, AcctPay, InvtAsset, etc.",
          "type": "any",
          "dimensionality": "scalar",
          "optional": true
        }
      ],
      "options": {
        "stream": false,
        "cancelable": false,
        "volatile": false
